config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "PART",
"language": "GDScript",
"path": "res://scripts/PART.gd"
} ]
_global_script_class_icons={
"PART": ""
}

[application]

config/name="novemberdev_endlessrunner"
run/main_scene="res://scenes/WORLD.tscn"
